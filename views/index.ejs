<!-- This is Slider Section -->
<section id="slider">
    <div id="carouselExampleCaptions" class="carousel slide carousel-fade" data-bs-ride="carousel">
        <div class="carousel-inner">
            <% imgrecord.forEach((slide, index) => { %>
                <div class="carousel-item <%= index === 0 ? 'active' : '' %>" data-interval="2000">
                    <img src="/slider/<%= slide.img1 %>" class="d-block w-100" alt="Slide <%= index + 1 %>">
                </div>
                <div class="carousel-item" data-interval="2000">
                    <img src="/slider/<%= slide.img2 %>" class="d-block w-100" alt="Slide <%= index + 2 %>">
                </div>
                <div class="carousel-item" data-interval="2000">
                    <img src="/slider/<%= slide.img3 %>" class="d-block w-100" alt="Slide <%= index + 3 %>">
                </div>
                <div class="carousel-item" data-interval="2000">
                    <img src="/slider/<%= slide.img4 %>" class="d-block w-100" alt="Slide <%= index + 4 %>">
                </div>
                <div class="carousel-item" data-interval="2000">
                    <img src="/slider/<%= slide.img5 %>" class="d-block w-100" alt="Slide <%= index + 5 %>">
                </div>
            <% }) %>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
    </div>
</section>






  
